{
    "name": "octavenz/reoako",
    "description": "A module for Reoako.",
    "type": "silverstripe-vendormodule",
    "version": "1.0.0",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0",
        "guzzlehttp/guzzle": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Octavenz\\Reoako\\": "code/"
        }
    },
    "extra": {
        "installer-name": "reoako",
        "expose": [
            "dist"
        ],
        "branch-alias": {
            "dev-master": " 1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
